p=tk.StringVar()
pas=tk.StringVar()
e1=tk.Entry(t,textvariable=n,font='GOUDY 20 bold')
e1.place(x=670,y=300)
e2=tk.Entry(t,textvariable=e,font='GOUDY 20 bold')
e2.place(x=670,y=360)
e3=tk.Entry(t, textvariable=p,font='GOUDY 20 bold')
e3.place(x=670,y=420)
e4=tk.Entry(t, textvariable=pas,font='GOUDY 20 bold')
e4.place(x=670,y=480)
sign=tk.Button(t,text='Sign Up',fg='white',bg='black',command=sign_up,font='GOUDY 20 bold')
sign.place(x=670,y=540)
log_ask=tk.Frame(t,bg='black')
log_text=tk.Label(log_ask,text='Already sign in --',fg='white',bg='black',font='GOUDY 15 underline')
log_text.pack(side='left')
log_sign=tk.Button(log_ask,text='Log In',fg='white',bg='black',command=log,font='GOUDY 15 bold ')
log_sign.pack(side='right')
log_ask.place(x=670,y=600)
t.mainloop()                  #creating infinite loop to run tk window
