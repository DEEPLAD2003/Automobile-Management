               def repair():                                #repair function 
                    global t
                    global label
                    label.destroy()
                    def back01():
                         global label
                         name_entry.destroy()
                         city_entry.destroy()
                         car_name.destroy()
                         city.destroy()
                         search.destroy()
                         back1.destroy()
                         label=tk.Label(t,text='WELCOME TO HYUNDAI',bg='black',fg='white',font='GOUDY  50 underline')
                         label.place(x='450',y='275')
                    def Search():
                         if len(name_entry.get()) != 0 and len(city_entry.get())!= 0:
                              loc=location.get()
                              cur.execute('select NAME from repair where LOCATION=(%s)',(loc,))
                              data=cur.fetchone()
                              text.insert('end','Our Service Centers At ' +loc+'---\n')
                              text.insert('end',data[0]+'\n')
                              text.pack(side='bottom')
                              ob.commit()
                              search['state']='disable'
                              def book_car():
                                   yesno=msg.askyesno('CAR SERVICE','Do You Really Want To Book A Service')
                                   name_entry.destroy()
                                   city_entry.destroy()
                                   car_name.destroy()
                                   city.destroy()
                                   text.destroy()
                                   book.destroy()
                                   search.destroy()
                                   back1.destroy()
                                   def done_():
                                        if len(e2.get())!=0:
                                             try:
                                                  cur.execute('select * from repair_booking')
                                                  booking_data=cur.fetchall()
                                                  ob.commit()
                                                  cur.execute('insert into repair_booking values(%s)',(e2.get(),))
                                                  ob.commit()
                                                  msg.showinfo('CAR SERVICE','Your Booking Done')
                                                  e1.destroy()
                                                  e2.destroy()
                                                  Done.destroy()
                                                  date.destroy()
                                                  time.destroy()
                                                  global label
                                                  label=tk.Label(t,text='WELCOME TO HYUNDAI',bg='black',fg='white',font='GOUDY  50 underline')
                                                  label.place(x='450',y='275')
                                             except:
                                                  msg.showerror('Error','Service is not available for your entered date and time')
                                        else:
                                             msg.showerror('Error','Please add Date and Time')
                                   date=tk.Label(t,text='Enter Date and Time in the Format Given Below',bg='black',fg='white',font='GOUDY 25')
                                   date.place(x=400,y=100)
                                   time=tk.Label(t,text='YYYYMMDDHHMMSS',bg='black',fg='white',font='GOUDY 20')
                                   time.place(x=400,y=200)
                                   Done=tk.Button(t,text='Done',command=done_,bg='yellow',fg='black',font='GOUDY 15')
                                   Done.place(x=400,y=250)
                                   time_=tk.StringVar()
                                   e2=tk.Entry(t,font='GOUDY 20',textvariable=time_)
                                   e2.place(x=690,y=205)
                         else:
                              msg.showerror('Error','Please add Car_Name and your City') 
                         book=tk.Button(t,text='Book a Service',font='GOUDY 10 bold',bg='yellow',fg='black',command=book_car)
                         book.place(x=1040,y=720)
                    car=tk.StringVar()
                    location=tk.StringVar()
                    car_name=tk.Label(t,text='Enter your Car Name',bg='black',fg='white',font='GOUDY 20')
                    car_name.place(x=400,y=100)
                    city=tk.Label(t,text='Enter your City',bg='black',fg='white',font='GOUDY 20')
                    city.place(x=400,y=160)
                    name_entry=tk.Entry(t,font='GOUDY 20',textvariable=car)
                    name_entry.place(x=680,y=100)
                    city_entry=tk.Entry(t,font='GOUDY 20',textvariable=location)
                    city_entry.place(x=680,y=160)
                    search=tk.Button(t,text='Seacrh',command=Search,bg='black',fg='white',font='GOUDY 12 bold')
                    search.place(x=680,y=220)
                    text=tk.Text(t,bg='dark blue',fg='white',font='GOUDY 15 bold',pady=5,height=5,width=52)
                    back1=tk.Button(t,text='Back',command=back01,bg='black',fg='white',font='GOUDY 12 bold')
                    back1.place(x=600,y=220)
               def web_search():                  #function for searching on webbrowser(opens directly browser)
                    webbrowser.open('https://www.google.com')
               menu1=tk.Menu(t)                                  #creating menus for tk window
               my_menu1=tk.Menu(menu1,tearoff=0)  
               my_menu_1=tk.Menu(my_menu1,tearoff=0)
               my_menu_1.add_command(label='Santro',command=santro)
               my_menu_1.add_command(label='Grand i10',command=grand_i10)
               my_menu_1.add_command(label='Grand i10 NIOS',command=grand_i10_nios)
               my_menu_1.add_command(label='Elite i20',command=elite_i20)
               my_menu_1.add_command(label='Aura',command=aura)
               my_menu_1.add_command(label='Xcent',command=xcent)
               my_menu_1.add_command(label='Verna',command=verna)
               my_menu_1.add_command(label='Elentra',command=elentra)
               my_menu_1.add_command(label='Venue',command=venue)
               my_menu_1.add_command(label='Creta',command=creta)
               my_menu_1.add_command(label='Tucson',command=tucson)
               my_menu_1.add_command(label='Kona Electric',command=kona_elec)
               my_menu1.add_cascade(label='HYUNDAI',menu=my_menu_1)
               menu1.add_cascade(label='New Car',menu=my_menu1)
               menu1.add_command(label='Repair',command=repair)
               menu1.add_command(label='Web Search',command=web_search)
               menu1.add_command(label='Exit',command=t.destroy)
               t.config(menu=menu1)     
          except:
               msg.showerror(title='ERROR',message='An Error Occured . Please check your details')
     else:
          msg.showerror('ERROR','Please check again your email-id and phone no.')





def log():                                   #Login function
     title_sign.destroy()
     inputs_sign.destroy()
     e1.destroy()
     e2.destroy()
     e3.destroy()
     e4.destroy()
     sign.destroy()
     log_ask.destroy()
     l.destroy()
     log_sign.destroy()
     def log_in():
          cur.execute('select e_mail_id,password from user_info' )
          d=cur.fetchall()
          e_mail=[]
          password=[]
          for i in d:
               e_mail.append(i[0])
               password.append(i[1])
          if ((user_input1.get() in e_mail and user_input2.get() in password)):
               i1=e_mail.index(user_input1.get())
               i2=password.index(user_input2.get())
               if i1==i2: 
                    msg.showinfo(title='Status',message='Log In Successfull')
                    title.destroy()
                    user_input1.destroy()
                    user_input2.destroy()
                    inputs.destroy()
                    passwd.destroy()
                    log.destroy()
                    global label
                    label=tk.Label(t,text='WELCOME TO HYUNDAI',bg='black',fg='white',font='GOUDY  50 underline')
                    label.place(x='450',y='275')

                    def santro(name='Santro'):                        #function for getting details of cars and thier images
                         def buy():
                              mg=msg.askyesno('Buy','Do You Want To Buy This Car')
                              if mg:
                                   message='You Owned '+name+' car'
                                   msg.showinfo('Buy',message)
                                   bu1['state']='disable'
                         def cart():
                              msg.showinfo('cart','Successfully added to cart')
                              bu2['state']='disable'
                         label.destroy()
                         cur.execute('select * from hyundai where car_name=%s',(name,))
                         text=cur.fetchone()
                         name_title=tk.Frame(t,bg='black')
                         tk.Label(name_title,text=name,font='GOUDY 50 bold',fg='yellow',bg='black').pack()
                         name_title.pack()
