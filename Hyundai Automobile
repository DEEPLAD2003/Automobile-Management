# Automobile-Management
The application is an user interactive based made using tkinter in python. 
import tkinter as tk                                                       #importing all required modules form python
from tkinter import ttk
import mysql.connector as sq
import tkinter.messagebox as msg
from PIL import Image,ImageTk
from tkinter import PhotoImage
import random
import urllib
import webbrowser
t=tk.Tk()
ob=sq.connect(host='localhost',user='root',password='bermu',database='deep')
cur=ob.cursor()
t.wm_iconbitmap("favicon.ico")                    #Giving icon to tkinter window
t.geometry('1500x750+15+15')
t.resizable(False,False)
t.title('HYUNDAI MOTORS')
t['bg']='black'
def sign_up():                                              #Sign up Function
     if (('@' in e.get() and '.com' in e.get()) and len(p.get())==10):
     

          try:
               cur.execute('insert into user_info values (%s,%s,%s,%s)',(n.get(),e.get(),p.get(),pas.get()))
               ob.commit()
               msg.showinfo(title='Status',message='Sign Up Successfull')
               global title_sign
               global inputs_sign
               global e1
               global l
               global e2
               global e3
               global e4
               global sign
               global log_ask
               
               title_sign.destroy()
               inputs_sign.destroy()
               e1.destroy()
               e2.destroy()
               e3.destroy()
               e4.destroy()
               l.destroy()
               sign.destroy()
               log_ask.destroy()
               global label
               label=tk.Label(t,text='WELCOME TO HYUNDAI',bg='black',fg='white',font='GOUDY  50 underline')
               label.place(x='450',y='275')
               def santro(name='Santro'):                                  #Here are function for getting details of cars and theirs images.
                    label.destroy()
                    def buy():
                         mg=msg.askyesno('Buy','Do You Want To Buy This Car')
                         if mg:
                              message='You Owned '+name+' car'
                              msg.showinfo('Buy',message)
                              bu1['state']='disable'
                    def cart():
                         msg.showinfo('cart','Successfully added to cart')
                         bu2['state']='disable'
                    label.destroy()
                    cur.execute('select * from hyundai where car_name=%s',(name,))
                    text=cur.fetchone()
                    name_title=tk.Frame(t,bg='black')
                    tk.Label(name_title,text=name,font='GOUDY 50 bold',fg='yellow',bg='black').pack()
                    name_title.pack()
                    details=tk.Frame(t,bg='black',padx=50,pady=50)
                    textbox=tk.Text(details,bg='dark blue',fg='white')
                    specification=['Car','Brand','Price','Bodystyle','Layout','Class','Engine','Transmission','Length','Breadth','Height','Wheelbase','Seating Capacity']
                    for i in range(0,len(specification)):
                         textbox.insert('end',specification[i]+' : '+text[i]+'\n')
                         textbox.pack()
                    details.pack(side='left')
                    frame_image=tk.Frame(t,borderwidth=10,padx=100,pady=30,bg='black')
                    textbox2=tk.Text(frame_image)
                    image_name=str(name)+'.jpg'
                    image=Image.open(image_name)
                    photo=ImageTk.PhotoImage(image)
                    img=tk.Label(textbox2,image=photo,bg='dark blue',fg='white')
                    img.image=photo
                    img.pack()
                    bu1=tk.Button(frame_image,text='BUY NOW',command=buy,bg='orange',fg='black')
                    bu1.pack(side='bottom',fill='x')
                    bu2=tk.Button(frame_image,text='ADD TO CART',command=cart,bg='orange',fg='black')
                    bu2.pack(side='bottom',fill='x')
                    textbox2.pack()
                    frame_image.pack(side='right')
                    def exit_():
                         global label
                         name_title.destroy()
                         details.destroy()
                         frame_image.destroy()
                         exit_frame.destroy()
                         label=tk.Label(t,text='WELCOME TO HYUNDAI',bg='black',fg='white',font='GOUDY  50 underline')
                         label.place(x='450',y='275')
                    exit_frame=tk.Frame(t)
                    tk.Button(exit_frame,text='Back',command=exit_,bg='yellow',fg='black',font='GOUDY 8 bold').pack()
                    exit_frame.place(x=100,y=100)
               def grand_i10(name='Grand i10'):
                    label.destroy()
                    def buy():
                         mg=msg.askyesno('Buy','Do You Want To Buy This Car')
                         if mg:
                              message='You Owned '+name+' car'
                              msg.showinfo('Buy',message)
                              bu1['state']='disable'
                    def cart():
                         msg.showinfo('cart','Successfully added to cart')
                         bu2['state']='disable'
                    label.destroy()
                    cur.execute('select * from hyundai where car_name=%s',(name,))
                    text=cur.fetchone()

                    name_title=tk.Frame(t,bg='black')
                    tk.Label(name_title,text=name,font='GOUDY 50 bold',fg='yellow',bg='black').pack()
                    name_title.pack()
                    details=tk.Frame(t,bg='black',padx=50,pady=50)
                    textbox=tk.Text(details,bg='dark blue',fg='white')
                    specification=['Car','Brand','Price','Bodystyle','Layout','Class','Engine','Transmission','Length','Breadth','Height','Wheelbase','Seating Capacity']
                    for i in range(0,len(specification)):
                         textbox.insert('end',specification[i]+' : '+text[i]+'\n')
                         textbox.pack()
                    details.pack(side='left')
                    frame_image=tk.Frame(t,borderwidth=10,padx=100,pady=30,bg='black')
                    textbox2=tk.Text(frame_image)
                    image_name=str(name)+'.jpg'
                    image=Image.open(image_name)
                    photo=ImageTk.PhotoImage(image)
                    img=tk.Label(textbox2,image=photo,bg='dark blue',fg='white')
                    img.image=photo
                    img.pack()
                    bu1=tk.Button(frame_image,text='BUY NOW',command=buy,bg='orange',fg='black')
                    bu1.pack(side='bottom',fill='x')
                    bu2=tk.Button(frame_image,text='ADD TO CART',command=cart,bg='orange',fg='black')
                    bu2.pack(side='bottom',fill='x')
                    textbox2.pack()
                    frame_image.pack(side='right')
                    def exit_():
                         global label
                         name_title.destroy()
                         details.destroy()
                         frame_image.destroy()
                         exit_frame.destroy()
                         label=tk.Label(t,text='WELCOME TO HYUNDAI',bg='black',fg='white',font='GOUDY  50 underline')
                         label.place(x='450',y='275')
                    exit_frame=tk.Frame(t)
                    tk.Button(exit_frame,text='Back',command=exit_,bg='yellow',fg='black',font='GOUDY 8 bold').pack()
                    exit_frame.place(x=100,y=100)
